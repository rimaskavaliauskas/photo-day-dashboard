{
  "name": "photo-day-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create photo-day-db",
    "db:migrate": "wrangler d1 execute photo-day-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute photo-day-db --local --file=./schema.sql",
    "tail": "wrangler tail"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "typescript": "^5.4.5",
    "wrangler": "^4.56.0"
  }
}
